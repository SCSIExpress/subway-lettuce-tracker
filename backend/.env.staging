# Staging Environment Variables
NODE_ENV=staging
PORT=5000

# Database Configuration
DATABASE_URL=postgresql://username:password@staging-db:5432/subway_lettuce_tracker_staging
DATABASE_SSL=true
DATABASE_POOL_MIN=1
DATABASE_POOL_MAX=10

# Redis Configuration
REDIS_URL=redis://staging-redis:6379
REDIS_PASSWORD=
REDIS_DB=0

# CORS Configuration
FRONTEND_URL=https://staging.your-domain.com

# Google Maps API
GOOGLE_MAPS_API_KEY=your_staging_google_maps_api_key_here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=200

# Security
JWT_SECRET=your_staging_jwt_secret_here
BCRYPT_ROUNDS=10

# Monitoring
ENABLE_PERFORMANCE_MONITORING=true
LOG_LEVEL=debug

# Cache Settings
CACHE_TTL=60
CACHE_WARMING_INTERVAL=900