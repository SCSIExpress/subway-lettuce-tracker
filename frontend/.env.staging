# Staging Environment Variables
VITE_API_URL=https://staging-api.your-domain.com/api
VITE_GOOGLE_MAPS_API_KEY=your_staging_google_maps_api_key_here
VITE_APP_NAME=Subway Lettuce Tracker (Staging)
VITE_APP_VERSION=1.0.0-staging
VITE_ENVIRONMENT=staging

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Map Configuration
VITE_DEFAULT_MAP_ZOOM=12
VITE_MAX_SEARCH_RADIUS=50000