# Staging Environment Template
# Copy this file to .env.staging and fill in the actual values

# Database Configuration
POSTGRES_USER=staging_postgres_user
POSTGRES_PASSWORD=staging_postgres_password

# Application URLs
STAGING_FRONTEND_URL=https://staging.your-domain.com
STAGING_VITE_API_URL=https://staging-api.your-domain.com/api

# Google Maps API Keys
STAGING_GOOGLE_MAPS_API_KEY=your_staging_google_maps_api_key

# Security
STAGING_JWT_SECRET=your_staging_jwt_secret_at_least_32_characters_long